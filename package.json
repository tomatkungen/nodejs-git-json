    "graphql": "^16.9.0",
    "nodegit": "0.28.0-alpha.28",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "nodegit": "0.28.0-alpha.28"
